panel.cor <- function(x , y , ...) {
par(usr = c(0 ,1,0 ,1 ))
r <-cor(x , y )
txt <- format (r, digits = 2 )
text( 0.5 , 0.5 , txt , cex = 1.5 )
}
panel.hist <- function(x , ...) {
usr <- par("usr")
par(usr = c( usr [ 1 : 2 ] , 0 , 1.5 ) )
h <- hist (x , plot = FALSE )
breaks <- h$breaks
nB <- length(breaks)
y <- h$counts
y <- y/max(y)
rect(breaks[- nB] , 0 , breaks[- 1] ,y,
col = "lightgrey" , ...)
box ()
}
